<html>
  <head>
    <title>Mini Message Board | The Odin Project</title>
    <link rel="stylesheet" href="style.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% messages.forEach(message => { %>
      <li>
        <p><%= message.user %></p>
        <p><%= message.text %></p>
        <p>
          <time datetime="<%= message.added %>"
            ><%= message.added.toLocaleString() %></time
          >
        </p>
      </li>
      <% }); %>
    </ul>
  </body>
</html>
