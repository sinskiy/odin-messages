<html>
  <head>
    <title>Mini Message Board | The Odin Project</title>
    <link rel="stylesheet" href="style.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% messages.forEach(message => { %>
      <%- include('message', { message }) %>
      <% }); %>
    </ul>
  </body>
</html>
